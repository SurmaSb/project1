pipeline {
    agent any

    environment {
        CI = 'true'
    }

    stages {
        stage('Build') {
            steps {
                sh 'npm install'
            }
        }

        stage('Deliver') { 
            steps {
           sh 'pkill -F /var/lib/jenkins/workspace/project1_project1_master-NKIZ7GAI62VYEDYVGYRYJWMHZNDS43UP4Q FFX6IAL6A2WNYIZHAQ'  
               sh 'set -x'
                sh 'nohup npm start > log.txt&'
                sh 'sleep 1'
                sh 'echo $! > .pidfile'
                sh 'set +x'
            }
        }
    }
}
