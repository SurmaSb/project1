pipeline {
    agent any

    environment {
        CI = 'true'
    }

    stages {
        stage('Build') {
            steps {
                sh 'npm install'
            }
        }

        stage('Deliver') { 
            steps {
                bash './jenkins/scripts/kill.sh'
                input message: 'Finished using the web site? (Click "Proceed" to continue)'
                bash './jenkins/scripts/deliver.sh'
            }
        }
    }
}

